    var $this = $(this);
    $this.addClass("active");
    $this.closest(".signupForm").find(".signup-email-modal-signup-not-valid").fadeOut(500);
  });
  $(".signup-email-modal-signup").on("keyup", function() {
    var $this = $(this);
